{
  "name": "webpack-react-redux",
  "version": "0.0.2",
  "description": "A starting point for projects with webpack, react and redux",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dist": "webpack",
    "lint": "./node_modules/.bin/eslint . --ext .js; exit 0",
    "start": "webpack-dev-server --config webpack.config.js --progress --colors --history-api-fallback",
    "storybook": "start-storybook -p 9001 & open http://localhost:9001",
    "pre-push": "npm test && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LittleFurryBastards/webpack-react-redux.git"
  },
  "keywords": [
    "webpack",
    "babel",
    "react"
  ],
  "author": "Deyan Yanakiev <kliomba@abv.bg>",
  "license": "MIT",
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/webpack-babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "dependencies": {
    "axios": "0.11.0",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-router": "2.1.1",
    "redux": "3.5.1"
  },
  "devDependencies": {
    "@kadira/storybook": "1.20.0",
    "babel-core": "6.7.6",
    "babel-eslint": "6.0.2",
    "babel-jest": "10.0.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "captain-git-hook": "1.1.1",
    "coveralls": "2.11.9",
    "css-loader": "0.23.1",
    "eslint": "2.8.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.3.0",
    "file-loader": "0.8.5",
    "jest-cli": "0.10.2",
    "less": "2.6.1",
    "less-loader": "2.2.3",
    "react-addons-test-utils": "15.0.1",
    "react-hot-loader": "1.3.0",
    "style-loader": "0.13.1",
    "webpack": "1.12.15",
    "webpack-babel-jest": "1.0.4",
    "webpack-dev-server": "1.14.1"
  }
}
